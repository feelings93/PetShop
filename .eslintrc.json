{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "warn",
    "arrow-body-style": "off",
    "react/jsx-filename-extension": "off",
    "react/function-component-definition": "off",
    "no-console": "off",
    "import/prefer-default-export": "off"
  }
}
